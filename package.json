{
  "name": "@scaleway/form",
  "version": "1.8.2",
  "description": "Scaleway Form",
  "homepage": "https://github.com/scaleway/scaleway-form#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/scaleway/scaleway-form"
  },
  "keywords": [
    "react",
    "reactjs",
    "ui"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.x",
    "yarn": ">=3.x"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "packageManager": "yarn@3.1.1",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "build:profile": "cross-env PROFILE=true yarn run build",
    "build:storybook": "STORYBOOK_ENVIRONMENT=production build-storybook",
    "check:deps": "npx depcheck . --skip-missing=true --ignores='bin,eslint,webpack,jest,husky,@semantic-release/*,@commitlint/*,@babel/*,babel-*'",
    "commit": "npx git-cz -a",
    "start": "STORYBOOK_ENVIRONMENT=development start-storybook -p 6006",
    "start:production": "STORYBOOK_ENVIRONMENT=production start-storybook -p 6006",
    "format": "prettier --write '**/*.{ts,tsx}'",
    "lint:fix": "yarn run lint --fix",
    "lint": "eslint --cache --ext ts,tsx .",
    "prebuild": "shx rm -rf dist",
    "test:unit:coverage": "yarn run test:unit --coverage",
    "test:watch": "yarn run test --watch",
    "test:update": "yarn run test --updateSnapshot",
    "test": "LC_ALL=en_US.UTF-8 jest",
    "test:unit": "yarn test",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --write",
      "eslint --fix --ext ts,tsx"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/mocks/*.{ts,tsx}",
      "!src/**/__stories__/**/*.{ts,tsx}",
      "!src/**/__tests__/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": ".reports",
          "outputName": "tests.xml"
        }
      ]
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@scaleway)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ]
  },
  "peerDependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@scaleway/ui": "0.135.2",
    "final-form": "4.20.6",
    "final-form-arrays": "3.0.2",
    "final-form-focus": "1.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-final-form": "6.5.8"
  },
  "devDependencies": {
    "@babel/core": "7.17.2",
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@emotion/eslint-plugin": "11.7.0",
    "@emotion/jest": "11.7.1",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-url": "6.1.0",
    "@scaleway/eslint-config-react": "3.3.7",
    "@scaleway/jest-helpers": "1.1.6",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@storybook/addon-actions": "6.4.18",
    "@storybook/addon-docs": "6.4.18",
    "@storybook/addon-essentials": "6.4.18",
    "@storybook/addon-links": "6.4.18",
    "@storybook/addons": "6.4.18",
    "@storybook/builder-webpack5": "6.4.18",
    "@storybook/manager-webpack5": "6.4.18",
    "@storybook/react": "6.4.18",
    "@storybook/theming": "6.4.18",
    "@testing-library/dom": "8.11.3",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/final-form-focus": "1.1.2",
    "@types/react-dom": "17.0.11",
    "babel-loader": "8.2.3",
    "babel-plugin-annotate-pure-calls": "0.4.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "date-fns": "2.28.0",
    "eslint": "8.8.0",
    "eslint-plugin-mdx": "1.16.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "jest-junit": "13.0.0",
    "lint-staged": "12.3.3",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "read-pkg": "7.0.0",
    "regenerator-runtime": "0.13.9",
    "rollup": "2.66.1",
    "rollup-plugin-dts": "4.1.0",
    "rollup-plugin-visualizer": "5.5.4",
    "semantic-release": "18.0.1",
    "typescript": "4.5.5",
    "webpack": "5.68.0"
  },
  "dependencies": {
    "@babel/runtime": "7.17.2",
    "@scaleway/ui": "0.135.2",
    "final-form": "4.20.6",
    "final-form-arrays": "3.0.2",
    "final-form-focus": "1.1.2",
    "react-final-form": "6.5.8"
  },
  "dependenciesMeta": {
    "@react-spring/core": {
      "built": false
    }
  },
  "contributors": [
